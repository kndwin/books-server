// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Book {
	id String @id @default(cuid())
	title String
	author String
	description String?
	publisher String?
	publishedDate String?
	pageCount Int?
	imageLink String?
	amount Int
}
